{
  "manifest_version": 3,
  "name": "Floating Button Extension",
  "version": "1.0",
  "description": "Chrome extension with a floating button",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "button.html",
    "default_icon": {
    }
  },
  "icons": {
  }
}
