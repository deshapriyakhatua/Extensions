{
    "name" : "Alliance LogIn",
    "version" : "1.1",
    "manifest_version" : 3,
    "action" : {
        "default_popup" : "popup.html"
    },
    "permissions" : [
        "activeTab",
        "scripting",
        "tabs",
        "notifications",
        "http://*/*",
        "https://*/*"
    ],
    "host_permissions" : [
        "<all_urls>"
    ]
    
}