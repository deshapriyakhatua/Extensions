{
    "manifest_version": 3,
    "name": "URL lists",
    "version": "1.0",
    "description": "Tracks and lists URLs opened today.",
    "permissions": [
      "scripting",
      "storage",
      "activeTab",
      "tabs"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
    
      }
    },
    "icons": {
      
    }
  }
  

  